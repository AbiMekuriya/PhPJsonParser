{
    "name": "abimekuriya/php-json-parser",
    "type": "library",
    "description": "Reusable php library for parsing json files and array data!",
    "keywords": [
        "abimekuriya",
        "php-json-parser"
    ],
    "homepage": "https://github.com/abimekuriya/php-json-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Abimalek Mekuriya",
            "email": "abi.mek2005@gmail.com",
            "homepage": "https://github.com/abimekuriya",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "abimek\\PhPJsonParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "abimek\\PhPJsonParser\\Test\\": "test"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
